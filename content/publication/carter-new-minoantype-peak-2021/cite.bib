@article{carterNewMinoantypePeak2021,
 abstract = {A long-recognised characteristic of Crete's later Bronze Age [BA] state-level society--the `Minoan civilization'of the 2nd millennium cal. BC--was the establishment of socio-economic connections with off-island populations. The nature of these relationships has been interpreted in various ways, from the establishment of overseas colonies to a more mutually beneficial relationship between local political agents and their Cretan partners, not least Knossos. Minoan influence has been documented throughout the southern Aegean in the \dots},
 author = {Carter, Tristan and Mallinson, Kristine and Mastrogiannopoulou, Vagia and Contreras, Daniel A and Diffey, Charlotte and Lopez, Claudette and Pareja, Marie N and Tsartsidou, Georgia and Athanasoulis, Dimitris},
 journal = {Journal of Greek Archaeology},
 pages = {60--100},
 title = {A New Minoan-type Peak Sanctuary on Stelida, Naxos},
 volume = {6},
 year = {2021}
}
